{
    "name": "akawalko/redbean-extras",
    "description": "This repository contains extra resources that complement the RedBeanPHP, a ORM layer that creates models, config and database on the fly.",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Arkadiusz KawaÅ‚ko"
        }
    ],
    "require": {
        "php": ">=7.4",
        "hanneskod/classtools": "^1.2",
        "psr/cache": "^1.0",
        "symfony/cache-contracts": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "gabordemooij/redbean": "^5.7",
        "symfony/cache": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Akawalko\\RedbeanExtras\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akawalko\\RedbeanExtras\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "gabordemooij/redbean": "ORM layer that creates models, config and database on the fly"
    }
}
